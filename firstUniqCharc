def firstUniqChar(s):
    d={}
    for i in range(len(s)):
        if s[i] in d:
            d[s[i]]+=1
        else:
            d[s[i]]=1
    for key,value in d.item():
        if value==1:
            return s.index(key)
    return-1
S=input("Enter the string: ")
print(firstUniqChar(s))     
            
